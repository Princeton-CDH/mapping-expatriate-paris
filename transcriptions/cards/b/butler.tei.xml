    <text>
        <body>
            
            <div type="card">
                <div type="recto">
                    <pb facs="#s1"/>
                    <head>
                        <date when="1939">1939</date>			<persName ref="#butl.h">BUTLER. H.M.</persName>
                        <address>
                            <name>Hotel de l'Univers</name>
                            <street>Rue Monsieur la Prince</street>
                        </address>
                    </head> 
                    <note>
                        1 m. 1 v. B.					50
                        dep			ref.		50
                    </note>
                    
                        
                    <ab ana="#borrowingEvent">
                        <date ana="#checkedOut" when="1939-07-05">july 5</date>	
                        <bibl ana="#borrowedItem" corresp="mep:001c2k">
                            <title>Apes of God.</title>
                        </bibl>					
                        <date ana="#returned" when="1939-07-06">july 6</date>
                    </ab>
                    
                    <ab ana="#borrowingEvent">
                        <date ana="#checkedOut" when="1939-07-06">6</date>	
                        <bibl ana="#borrowedItem" corresp="mep:00088c">
                            <title>New Writing 2.</title>
                            <biblScope unit="issue">spring 1939.</biblScope>
                        </bibl>		
                        <date ana="#returned" when="1939-07-07">july 7</date>
                    </ab>
                    
                   <ab ana="#borrowingEvent">
                       <date ana="#checkedOut" when="1939-07-07">7</date>	
                       <bibl ana="#borrowedItem" corresp="mep:003m0c">
                           <title>The wind in <unclear/></title>
                       </bibl>			
                       <date ana="#returned" when="1939-07-10">july 10</date>
                   </ab>
                    
                    <ab ana="#borrowingEvent">
                        <date ana="#checkedOut" when="1939-07-07">7</date>	
                        <bibl ana="#borrowedItem" corresp="mep:00080w">
                            <title>Escape to life</title>
                        </bibl>					
                        <date ana="#returned" when="1939-07-10">____ 10</date>
                    </ab>
                    
                    <ab ana="#borrowingEvent">
                        <date ana="#checkedOut" when="1939-07-10">10</date>	
                        <bibl ana="#borrowedItem" corresp="mep:003m1p">
                            <title>Naomi Mitchinson's Vienna Diary</title>
                        </bibl>	
                        <date ana="#returned" when="1939-07-13">July 13</date>
                    </ab>
                    
                    <ab ana="#borrowingEvent">
                        <date ana="#checkedOut" when="1939-07-10">10</date>	
                        <bibl ana="#borrowedItem" corresp="mep:003m20">
                            <title>Looking forward</title>
                        </bibl>					
                        <date ana="#returned" when="1939-07-13">July 13</date>
                    </ab>
                    
                    <ab ana="#borrowingEvent">
                        <date ana="#checkedOut" when="1939-07-13">13</date>	
                        <bibl ana="#borrowedItem" corresp="mep:001h1c">
                            <title>Starting Point</title>
                        </bibl>					
                        <date ana="#returned" when="1939-07-17">july 17</date>
                    </ab>
                    
                    <ab ana="#borrowingEvent">
                        <date ana="#checkedOut" when="1939-07-13">13</date>	
                        <bibl ana="#borrowedItem" corresp="mep:003m39">
                            <title>Revolution <unclear/></title>
                        </bibl>			
                        <date ana="#returned" when="1939-07-18">july 18</date>
                    </ab>
                    
                    <ab ana="#borrowingEvent">
                        <date ana="#checkedOut" when="1939-07-17">17</date>	
                        <bibl ana="#borrowedItem" corresp="mep:003m4m">
                            <title>Writer in a changing world</title>
                        </bibl>		
                        <date ana="#returned" when="1939-07-18">july 18</date>
