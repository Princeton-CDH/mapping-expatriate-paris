                    <ab ana="#borrowingEvent">
                        <date ana="#checkedOut" when="1939-07-18">18</date>
                        <bibl ana="#borrowedItem" corresp="mep:003m5x">
                            <title>Intelligentsia of Great Britain</title>, 
